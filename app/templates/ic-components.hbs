<section class="fixed-vertical flex flex-column" style="width: 240px;">
  <logo style="width: 100%;">
    {{#link-to 'index'}}
      {{ic-logo}}
    {{/link-to}}
  </logo>
  <nav class="component-menu" style="overflow-y: scroll;">
    <components style="display: block;">
      {{#each}}
        {{#link-to 'ic-components.component' this.slug tagName='component'}}
          &#123;&#123;{{name}}&#125;&#125;
        {{/link-to}}
      {{/each}}
    </components>           
  </nav>
</section>
<article class="fixed-vertical" style="right: 0; left: 240px; padding: 24px;">
  {{outlet}}
</article>